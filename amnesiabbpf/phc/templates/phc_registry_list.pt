<div tal:repeat="person data.get('people', ())" class="registry">
    <div class="gravatar">
        <a href="${request.resource_path(context, 'person', query={'id':person['id']})}"><img src="${h.gravatar(request, person['email'], 48)}" /></a>
    </div>
    <div class="person">${person['firstname']} ${person['familyname']}</div>
</div>
